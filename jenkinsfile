 pipeline {
     agent {lable 'linux }
     options {
       buildDiscarder(logRotator(numToKeepStr: '5'))
     }
     stages {
       stage('scan') {
         steps {
           withSonarQubeEnv(installationName: sq1) {
             sh './mvnw clean org.sonarsource.scanner.maven:sonar-maven-plugin:3.8.6:sonar'
            }
         }
      }
   }
}
